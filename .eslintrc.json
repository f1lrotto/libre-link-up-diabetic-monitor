{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": ["error", { "allow": ["info", "warn", "error"] }],
        "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1 }]
    }
}
